{
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "destructuring": true,
    "objectLiteralShorthandMethods": true
  },
  "env": {
    "es6": true,
    "jasmine": true,
    "node": true
  },
  "globals": {
    "jest": false
  },
  "rules": {
    "comma-dangle": [1, "always-multiline"],
    "no-unused-vars": [1, {"vars": "all", "args": "after-used"}],
    "no-var": 2,
    "prefer-const": 1,
    "semi": 2
  }
}
